init_config:

instances:
    -   host: localhost
        port: 7199
        name: jmx_instance
        user: username
        password: password
        domains:
            - name: first_domain
              beans:
                - name: first_bean
                  attributes:
                    - name: attribute1
                      type: counter
                    - name: attribute2
                      type: gauge
                - name: second_bean
                  attributes: all             
                  # In this case attributes will be considered as gauges

            - name: second_domain
              beans: all

    -   host: localhost
        port: 7200
        name: second_jmx_instance
        domains: all